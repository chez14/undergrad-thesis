@article{buchin:13:median,
	author		= {Kevin Buchin and Maike Buchin and Marc J. van Kreveld and Maarten L{\"{o}}ffler and Rodrigo I. Silveira and Carola Wenk and Lionov Wiratma},
	title     	= {Median Trajectories},
	journal   	= {Algorithmica},
	volume    	= {66},
	pages     	= {595--614},
	year      	= {2013},
}

%	4. Artikel dari prosiding seminar/konferensi
%	Artikel (atau paper) yang berasal dari prosiding seminar/konferensi ilmiah. 
%
%	Field yang wajib ada:
%		1. author		: penulis
%		2. title 		: judul artikel
%		3. pages		: halaman awal dan akhir dari artikel pd prosiding
%		4. booktitle	: nama prosiding
%		5. year			: tahun diadakannya seminar/konferensi
%		6. month		: tanggal awal-akhir dan bulan diadakannya seminar/konferensi
%		7. address		: tempat (kota dan negara tempat diadakannya seminar/konferensi)
%		8. publisher	: penerbit, disertai dengan lokasi penerbit (kota) !!
%
%		Note : penulisan month HARUS SETELAH year !
%
%	Contoh hasil di Daftar Referensi: 
%	van Kreveld, M. J. dan Wiratma, L. (2011) Median trajectories using well-visited regions and shortest paths. Proceedings of the 19th ACM SIGSPATIAL International Conference on Advances in Geographic Information Systems, Chicago, USA, 1-4 November, pp. 241-250. ACM, New York.
%
%	Contoh penulisan di file bib:

@inproceedings{kreveld:11:median,
	author    	= {Marc J. van Kreveld and Lionov Wiratma},
	title     	= {Median trajectories using well-visited regions and shortest paths},
	pages     	= {241--250}, 	
	booktitle 	= {Proceedings of the 19th ACM SIGSPATIAL International Conference on Advances in Geographic Information Systems},	
	year      	= {2011},
	month		= {1-4 November},
	address		= {Chicago, USA},
	publisher	= {ACM, New York}
}

